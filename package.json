  "scripts": {
    "start": "react-scripts start",
    "start:server": "node build/server.js",
    "build": "yarn build:client && yarn build:server",
    "build:client": "react-scripts build",
    "build:server": "NODE_ENV=production webpack --progress --mode production --config config/webpack.server.config.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "react-scripts test --coverage --env=jest-environment-jsdom-sixteen --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:local": "BROWSER_TEST_ENV=local yarn browser-test",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" testcafe/e2e/",
    "lint": "eslint --ext js,ts,tsx src",
    "format-code": "eslint --ext js,ts,tsx src --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
